<view>
  <swiper
  indicator-dots='{{indicatorDots}}' autoplay='{{autoplay}}' interval='{{interval}}' duration='{{duration}}' circular='{{duration}}' current='{{swiperCurrent}}' bindchange='swiperChange' class='swiper'>
  <block wx:for='{{imgUrls}}' wx:key='unique'>
    <swiper-item>
    <image src='{{item}}' class='img' bindtap='swipclick'></image>
    </swiper-item>
  </block>
  </swiper>
  </view>